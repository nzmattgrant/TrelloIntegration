<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <ADUsesOwinOrOpenIdConnect>False</ADUsesOwinOrOpenIdConnect>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://trellointegrationdemo.azurewebsites.net</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>trellointegrationdemo.scm.azurewebsites.net:443</MSDeployServiceURL>
    <DeployIisAppPath>TrelloIntegrationDemo</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>True</EnableMSDeployBackup>
    <UserName>$TrelloIntegrationDemo</UserName>
    <_SavePWD>True</_SavePWD>
    <_DestinationType>AzureWebSite</_DestinationType>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="TrelloIntegrationContext" Order="1" Enabled="True">
          <Destination Path="Data Source=trellointegration.database.windows.net;Initial Catalog=TrelloIntegration;Persist Security Info=False;User ID=matt;Password=TrelloIntegration1;Connect Timeout=30;Encrypt=True;TrustServerCertificate=False" Name="Server=tcp:trellointegration.database.windows.net,1433;Data Source=trellointegration.database.windows.net;Initial Catalog=TrelloIntegration;Persist Security Info=False;User ID=matt;Password=TrelloIntegration1;MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;" />
          <Object Type="DbCodeFirst">
            <Source Path="DBMigration" DbContext="TrelloIntegration.DAL.TrelloIntegrationContext, TrelloIntegration" MigrationConfiguration="TrelloIntegration.Migrations.Configuration, TrelloIntegration" Origin="Configuration" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)TrelloIntegrationContext-Web.config Connection String">
      <ParameterValue>Server=tcp:trellointegration.database.windows.net,1433;Data Source=trellointegration.database.windows.net;Initial Catalog=TrelloIntegration;Persist Security Info=False;User ID=matt;Password=TrelloIntegration1;MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
</Project>